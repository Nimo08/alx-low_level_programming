CC=gcc
SRC=main.c school.c
OBJ=$(SRC:.c=.o)
NAME=school
CFLAGS= -Wall -Werror -Wextra -pedantic
.PHONY: all clean oclean fclean re
all:$(NAME)
$(NAME): $(wildcard *.o) m.h
	$(CC) -Wall -Werror -Wextra -pedantic $(wildcard *.o) -o $(NAME)
clean:
	$(RM) *~ $(NAME)
oclean: 
	$(RM) *.o
fclean: clean oclean
re: oclean all
